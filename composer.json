{
    "name": "blockbrawn/commando",
    "description": "A Command Framework virion for PocketMine-MP ",
    "type": "library",
    "license": "LGPL-3.0-only",
    "minimum-stability": "dev",
    "require": {
        "blockbrawn/simple-packet-handler": "dev-pm5"
    },
    "require-dev": {
        "nethergamesmc/pocketmine-mp": "dev-stable"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:NetherGamesMC/PocketMine-MP.git"
        }
    ],
    "autoload": {
        "psr-4": {"CortexPE\\Commando\\": ["src/CortexPE/Commando/"]}
    },
    "extra": {
        "virion": {
            "spec": "3.0",
            "namespace-root": "CortexPE\\Commando"
        }
    }
}
